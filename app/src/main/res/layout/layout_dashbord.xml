<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/constrint_layout_id"
    android:background="#3F424B">


    <ImageView
        android:id="@+id/nav_drawer_icon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/drawerimageicon"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/stegofun_id"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_margin="50dp"
        android:background="@drawable/stegofun"
        app:layout_constraintBottom_toTopOf="@+id/sefeId"
        tools:layout_editor_absoluteX="50dp">
    </ImageView>

    <ImageView
        android:id="@+id/sefeId"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_margin="50dp"
        android:background="@drawable/cryptofun"
        android:paddingBottom="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </ImageView>

    <ImageView
        android:id="@+id/cornerLogoId"
        android:layout_width="120dp"
        android:layout_height="106dp"
        android:src="@drawable/androidimage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>